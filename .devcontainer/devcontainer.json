{
  "name": "MongoDB Lab Runner",
  "dockerComposeFile": "docker-compose.yml",
  "service": "lab-runner",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": [
    8888,
    27017
  ],
  "hostRequirements": {"cpus": 4},
  "containerEnv": {
    "SERVERLESS_URL": "https://vtqjvgchmwcjwsrela2oyhlegu0hwqnw.lambda-url.us-west-2.on.aws/"
  },	
	"postCreateCommand": "pip install -r /home/jovyan/requirements.txt && bash /home/jovyan/install-mongodb-db-tools.sh && bash /home/jovyan/import.sh",
  "customizations": {
    "codespaces": {"openFiles": []},
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-tools.vscode-jupyter",
        "ms-toolsai.jupyter",
        "mongodb.mongodb-vscode"
      ]
    }
  }
  //"remoteUser": "jovyan"
}