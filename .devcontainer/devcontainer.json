{
  "name": "MongoDB Lab Runner",
  "dockerComposeFile": "docker-compose.yml",
  "service": "lab-runner",
  "workspaceFolder": "/home/jovyan/work",
  "forwardPorts": [
    8888,
    27017
  ],
  "hostRequirements": {"cpus": 4},
  "containerEnv": {
    "SERVERLESS_URL": "https://vtqjvgchmwcjwsrela2oyhlegu0hwqnw.lambda-url.us-west-2.on.aws/"
  },
  "postCreateCommand": "pip install -r /home/jovyan/requirements.txt",
  "customizations": {
    "codespaces": {"openFiles": []},
    "vscode": {"extensions": ["ms-toolsai.jupyter", "ms-python.python"]},
  },
  "remoteUser": "jovyan",
  "mounts": [
    "type=bind,source=${localWorkspaceFolder},target=/home/jovyan/work,consistency=cached"
  ]
}